{
  "extends": [
    "react-app"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/no-children-prop": 0,
    "react/jsx-key": 0,
    "jsx-a11y/anchor-has-content": 0,
    "react/react-in-jsx-scope": 0
  }
}